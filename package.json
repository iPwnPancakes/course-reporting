{
    "name": "course-reporting",
    "version": "1.0.0",
    "description": "",
    "main": "src/bootstrap.ts",
    "scripts": {
        "start": "ts-node -r dotenv/config -r reflect-metadata src/bootstrap.ts",
        "build": "tsc --build",
        "clean": "tsc --build --clean",
        "test:acceptance": "cucumber-js --config=tests/Acceptance/cucumber.config.json",
        "test:use-case": "cucumber-js --config=tests/UseCase/cucumber.config.json",
        "test:unit": "mocha -r ts-node/register src/**/*.spec.ts",
        "test:contract": "mocha -r ts-node/register -r dotenv/config src/**/*.contract.ts",
        "typeorm": "typeorm-ts-node-commonjs",
        "typeorm:migrate": "typeorm-ts-node-commonjs migration:run -d src/Infrastructure/DatabaseConnection/TypeOrm/MigrationDataSource.ts",
        "typeorm:rollback": "typeorm-ts-node-commonjs migration:revert -d src/Infrastructure/DatabaseConnection/TypeOrm/MigrationDataSource.ts"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@cucumber/cucumber": "^8.10.0",
        "@faker-js/faker": "^7.6.0",
        "@types/chai": "^4.3.4",
        "@types/hapi": "^18.0.8",
        "@types/mocha": "^10.0.1",
        "@types/node": "^16.11.10",
        "chai": "^4.3.7",
        "mocha": "^10.2.0",
        "ts-mockito": "^2.6.1",
        "ts-node": "10.7.0",
        "typescript": "4.5.2"
    },
    "dependencies": {
        "@hapi/hapi": "^21.3.0",
        "dotenv": "^16.0.3",
        "mysql2": "^3.1.2",
        "reflect-metadata": "^0.1.13",
        "typeorm": "0.3.12"
    }
}
